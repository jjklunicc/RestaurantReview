<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="User">
	<typeAlias  alias="UserVO" type="egovframework.example.member.service.UserVO"/>

	<insert id="UserDAO.insertUser">
		<![CDATA[
		INSERT INTO User
		(   ID
		  , PW
		  , NAME
		  , ADDR
		  , PHONENUMBER
		  , CREATE_DT
		  , UPDATE_DT )
		VALUES (
		    #id#
		  , #pw#
		  , #name#
		  , #addr#
		  , #phonenumber#
		  , NOW()
		  , NOW() )
		]]>
	</insert>
	
